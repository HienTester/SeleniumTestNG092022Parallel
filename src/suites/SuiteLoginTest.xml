<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Suite Login Test" verbose="1">

    <listeners>
        <listener class-name="hientester.com.listeners.TestListener"/>
    </listeners>


    <test name="Login Test on edge" parallel="methods">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="hientester.com.testcases.LoginTest" />
        </classes>
    </test>

</suite>